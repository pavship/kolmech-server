    "playground": "graphql playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.16.0",
    "jsonwebtoken": "8.3.0",
    "node-fetch": "^2.2.0",
    "prisma-binding": "2.1.4"
  },
  "devDependencies": {
    "dotenv": "6.0.0",
    "graphql-cli": "2.16.5",
    "nodemon": "1.18.3",
    "now-env": "^3.1.0",
    "npm-run-all": "4.1.3",
    "prisma": "1.13.7"
  }
}
